{
  "name": "passable-react",
  "version": "0.7.5",
  "description": "Wrapper Component for react that allows easy Passable integration",
  "main": "./dist/passable-provider.js",
  "scripts": {
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "cross-env WEBPACK_ENV=build webpack",
    "build:watch": "webpack --watch",
    "pretest": "npm run build",
    "eslint": "eslint -c .eslintrc \"./src/**/*.js\"; exit 0"
  },
  "jest": {
    "setupFiles": [
      "./test/jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Evyatar Alush (evyatar.a@fiverr.com)",
  "license": "ISC",
  "devDependencies": {
    "@fiverr/eslint-config-fiverr": "1.0.0",
    "@storybook/react": "^3.2.17",
    "babel-jest": "^22.0.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.2.0",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.13.1",
    "jest": "^22.0.4",
    "passable": "^6.0.2",
    "react": "^16.6.3",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "uglifyjs-webpack-plugin": "^1.1.6"
  },
  "peerDependencies": {
    "passable": "^6.0.2"
  },
  "dependencies": {
    "lodash": "^4.17.10"
  }
}
